{
  "name": "symfony/scheduler",
  "type": "library",
  "description": "Symfony Scheduler Component",
  "keywords": ["schedule", "scheduler", "tasks", "cron", "automation"],
  "homepage": "https://symfony.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Guillaume Loulier",
      "email": "contact@guillaumeloulier.fr"
    },
    {
      "name": "Symfony Community",
      "homepage": "https://symfony.com/contributors"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "dragonmantank/cron-expression": "^3.0",
    "psr/log": "~1.0",
    "symfony/console": "^5.1",
    "symfony/event-dispatcher": "^5.1",
    "symfony/filesystem": "^5.1",
    "symfony/finder": "^5.1",
    "symfony/lock": "^5.1",
    "symfony/options-resolver": "^5.1",
    "symfony/process": "^5.1",
    "symfony/property-access": "^5.1",
    "symfony/serializer": "^5.1",
    "symfony/stopwatch": "^5.1"
  },
  "require-dev": {
    "ext-pcntl": "*",
    "ext-redis": "*",
    "symfony/dependency-injection": "^5.1",
    "symfony/http-client": "^5.1",
    "symfony/http-kernel": "^5.1",
    "symfony/mailer": "^5.1",
    "symfony/messenger": "^5.1",
    "symfony/notifier": "^5.1"
  },
  "suggest": {
    "symfony/event-dispatcher": "For dispatching and listening to tasks & worker lifecycle events",
    "symfony/http-client": "For dispatching http tasks",
    "symfony/mailer": "For sending emails according to task execution",
    "symfony/messenger": "For dispatching tasks to queue",
    "symfony/notifier": "For dispatching notification tasks and respond to task execution"
  },
  "autoload": {
    "psr-4": { "Symfony\\Component\\Scheduler\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "5.3-dev"
    }
  }
}
